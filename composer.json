{
    "name": "jlapp/swaggervel",
    "description": "A great way to integrate Swagger into Laravel",
    "keywords": ["laravel"],
    "version": "0.0.2",
    "license": "MIT",
    "authors": [
        {
            "name": "jlapp",
            "email": "jordan@jordanlapp.com"
        }
    ],
    "config": {
        "app-dir": "../../../app",
        "doc-dir": "../../../docs"
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "zircote/swagger-php": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Jlapp\\Swaggervel": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "Jlapp\\Swaggervel\\Updater::postUpdate"
        ]
    },
    "minimum-stability": "dev"
}
